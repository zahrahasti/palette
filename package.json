{
	"name": "palette",
	"version": "0.0.1",
	"private": true,
	"types": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint:fix": "eslint . --fix",
		"lint": "eslint --config eslint.config.js --ignore-pattern '.vscode/*.json' --fix --cache-location '/node_modules/.eslintcache/eslint .",
		"lint:prettier": "prettier --check .",
		"format": "npx prettier '**/*.{html,css,svelte,json}' --config prettier.config.js --write --cache",
		"json": "json-server --watch db.json --port 3000"
	},
	"devDependencies": {
		"@antfu/eslint-config": "^2.22.0",
		"@eslint/js": "^9.7.0",
		"@html-eslint/eslint-plugin": "^0.25.0",
		"@html-eslint/parser": "^0.25.0",
		"@stylistic/eslint-plugin": "^2.6.0-beta.0",
		"@stylistic/eslint-plugin-js": "^2.6.0-beta.0",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.27.4",
		"@types/color-name": "^1.1.3",
		"@types/node": "^20.14.10",
		"autoprefixer": "^10.4.17",
		"eslint": "^9.7.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-svelte": "^2.42.0",
		"eslint-plugin-tailwindcss": "^3.17.4",
		"globals": "^15.8.0",
		"postcss": "^8.4.35",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.6",
		"prettier-plugin-tailwindcss": "^0.6.5",
		"svelte": "^4.2.18",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"typescript-eslint": "^7.16.0",
		"vite": "^4.4.2"
	},
	"type": "module",
	"dependencies": {
		"-": "^0.0.1",
		"@types/randomcolor": "^0.5.9",
		"chroma-js": "^2.4.2",
		"eslint-plugin-svelte3": "^4.0.0",
		"json-server": "^1.0.0-beta.1",
		"randomcolor": "^0.6.2",
		"rollup-plugin-json": "^4.0.0"
	}
}
